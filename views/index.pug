extends layout

block content
  table(class='gridtable')
    tr
      th Id
      th UserName
      th Age
      th Email
      th UserLevel
      th q1
      th q2
      th q3
      th 
      th 
    each item in people
      tr
        td
          a(href='/details/' + item.id) #{item.id}
        td #{item.username}
        td #{item.age}
        td #{item.email}
        td #{item.userLevel}
        td #{item.q1textbox}
        td #{item.q2textbox}
        td #{item.q1textbox}
        td
          a(href='/edit/' + item.id) Edit
        td
          a(href='/delete/' + item.id) Delete
  p
  #menu
    a(href='/create') Add new user
    br
    br
    a(href='/logout') Logout